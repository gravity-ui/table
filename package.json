{
  "name": "@gravity-ui/table",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/esm/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gravity-ui/table"
  },
  "bugs": {
    "url": "https://github.com/gravity-ui/table/issues"
  },
  "homepage": "https://github.com/gravity-ui/table#readme",
  "scripts": {
    "build": "gulp",
    "build-storybook": "storybook build -c .storybook -o storybook-static",
    "clean": "gulp clean",
    "lint": "run-p lint:*",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:prettier": "prettier --check '**/*.{md,yaml,yml,json}'",
    "lint:prettier:fix": "prettier --write '**/*.{md,yaml,yml,json}'",
    "lint:styles": "stylelint 'src/**/*.scss' --report-needless-disables",
    "lint:styles:fix": "npm run lint:styles -- --fix",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "start": "storybook dev",
    "test": "jest",
    "test-storybook": "test-storybook",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bem-react/classname": "^1.6.0",
    "@tanstack/react-table": "^8.17.3",
    "@tanstack/react-virtual": "^3.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@gravity-ui/eslint-config": "^3.2.0",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.1",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@gravity-ui/uikit": "^6.14.1",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.1.1",
    "@storybook/react-webpack5": "^8.1.1",
    "@storybook/test-runner": "^0.18.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/jest": "^29.5.12",
    "@types/jest-image-snapshot": "^6.4.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "gulp-dart-sass": "^1.1.0",
    "gulp-replace": "^1.1.4",
    "gulp-typescript": "^5.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "jest-transform-css": "^6.0.1",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.5",
    "sass": "^1.77.1",
    "storybook": "^8.1.1",
    "stylelint": "^15.11.0",
    "stylelint-use-logical": "^2.1.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@dnd-kit/core": "^6.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@gravity-ui/uikit": "^5.0.0 || ^6.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "nano-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ],
    "*.{md,yaml,yfml,json}": [
      "prettier --write"
    ]
  }
}
